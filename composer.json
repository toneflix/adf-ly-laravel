{
    "name": "toneflix-code/adf-ly-laravel",
    "version": "1.0.1",
    "description": "A laravel package that wraps around Adf.ly API library.",
    "keywords": [
        "toneflix",
        "adf.ly",
        "laravel"
    ],
    "homepage": "https://github.com/toneflix/adf-ly-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Toneflix Code",
            "email": "code@toneflix.com.ng",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Toneflix\\AdfLy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Toneflix\\AdfLy\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Toneflix\\AdfLy\\AdfLyServiceProvider"
            ],
            "aliases": {
                "AdfLy": "Toneflix\\AdfLy\\AdfLyFacade"
            }
        }
    }
}
